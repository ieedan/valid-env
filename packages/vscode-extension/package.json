{
  "name": "venv-lang",
  "displayName": "venv-lang",
  "description": "Adds language support for valid-env",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "venv",
      "aliases": ["valid-env", "venv"],
      "extensions": [".venv"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "venv",
      "scopeName": ".venv",
      "path": "./syntaxes/venv.tmLanguage.json"
    }]
  }
}
